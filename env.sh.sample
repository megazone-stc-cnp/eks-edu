#!/bin/bash

# Profile 정보 없는 경우 주석처리
export AWS_PROFILE=<profile명>

export IDE_NAME=<사번>

export HOME_DIR=/home/ec2-user/environment/eks-edu
export EKS_VERSION=1.32
# =============================================================
# 자동 생성
export CLUSTER_NAME=eks-edu-cluster-${IDE_NAME}

export PROFILE_STRING=""
